   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "id": "BaCvQYBoEZWp"
   },
   "outputs": [],
   "source": [
    "n_points = 10\n",
    "x=np.linspace(0, 2*np.pi, n_points)\n",
    "f=interpolate.interp1d( x,np.sin(x),kind='linear' )"
   ]
